echantillion_X_Y <- function(p1,p2,N)
{
X <- rep(0,N)
for(i in 1:N)
{
u <- runif(1)
if(u <= p1)
X[i] = -1
else if(u<=p2+p1)
X[i] = 0
else
X[i]= 1
}
return(X)
}
trace_moyenne_empirique_X_Y<- function(p1,p2,N)
{
X <- rep(0,N)
for(i in 1:N)
{
X[i] <-mean(echantillion_X_Y(p1,p2,i))
}
return (X)
}
